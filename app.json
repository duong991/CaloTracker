{
  "name": "Calo Tracker",
  "displayName": "Calo Tracker",
  "expo": {
    "name": "Calo Tracker",
    "slug": "Calo Tracker",
    "scheme": "pizzaapp",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/images/logoApp.png",
    "splash": {
      "image": "./assets/images/logoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#143d54"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "icon": "./assets/images/logoApp.png",
      "package": "com.pizzaapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoApp.png",
        "backgroundImage": "./assets/images/logoApp.png"
      },
      "splash": {
        "image": "./assets/images/logoApp.png",
        "resizeMode": "contain",
        "backgroundColor": "#143d54"
      }
    },
    "ios": {
      "icon": "./assets/images/logoApp.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.pizzaapp",
      "splash": {
        "image": "./assets/images/logoApp.png",
        "tabletImage": "./assets/images/logoApp.png",
        "resizeMode": "contain",
        "backgroundColor": "#143d54"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#143d54"
      }
    }
  }
}
